                            <span class="logo">J&Q</span>
                            <span class="motto">We make websites.</span>
                          </div>
                        </div>
                      </div>

                      <!--SVG animation-->
                      <script>
                        var p = document.querySelector('.animate'),
                        offset = 600;
                        var offsetMe = function() {
                        if (offset < 0) {
                        offset = 600;
                        }
                        p.style.strokeDashoffset = offset;
                        offset = offset - 5;
                        requestAnimationFrame(offsetMe);
                        }
                        offsetMe();
                      </script>

                      <!--Desktop Navigation-->
                      <span class="desktop">
                        <header>
